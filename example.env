# Server
PORT=3000
NODE_ENV=development

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379

# Crawler
MAX_CONCURRENT_JOBS=5
MAX_DEPTH=3
USER_AGENT="Mozilla/5.0 (compatible; ResearchCrawler/1.0; +http://example.com/bot)"
REQUEST_TIMEOUT=15000
CACHE_TTL=3600

# Security
RATE_LIMIT_WINDOW=15
RATE_LIMIT_MAX=100
BLACKLISTED_DOMAINS="bad-domain.com,spam-site.org"